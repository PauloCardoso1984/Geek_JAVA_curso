package calculo;

public class Pitagoras {
	
	public int a;
	public int b;
	public int c;
		
	public Pitagoras(int a, int b, int c) {
		this.a = a;
		this.b = b;
		this.c = c;
	}

	public Pitagoras() {
	}

	public int getA() {
		return a;
	}
	public void setA(int a) {
		this.a = a;
	}
	public int getB() {
		return b;
	}
	public void setB(int b) {
		this.b = b;
	}
	public int getC() {
		return c;
	}
	public void setC(int c) {
		this.c = c;
	}
	
	// Metodo
	public void pitagora (int a, int b, int c) {
		Math.sqrt(Math.pow(a, 2) + Math.pow(b, 2) - Math.pow(2, 2));
	}
}
