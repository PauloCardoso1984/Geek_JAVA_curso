package object;

public class Cliente {
	String nome;
	String endereço;
	
	// CONSTRUTOR VAZIO
	public Cliente() {
	}

	// CONSTRUTOR COM ARGUMENTOS
	public Cliente(String nome, String endereço) {
		this.nome = nome;
		this.endereço = endereço;
	}

	@Override
	public String toString() {
		return "Cliente = " + nome + ", endereço = " + endereço;
	}	
}
